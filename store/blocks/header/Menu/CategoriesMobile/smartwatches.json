{
    "vtex.menu@2.x:submenu.accordion#smartwatches-mob": {
        "children":["menu#category-submenu-smartwatches-mobile"],
        "props": {
            "width": "100%",
            "orientation": "vertical"
        }
    },


    "disclosure-layout#smartwatches-mob":{
        "children": [
            "disclosure-trigger#smartwatches-trigger-mob",
            "disclosure-content#smartwatches-content-mob"
        ]
    },
    "disclosure-trigger#smartwatches-trigger-mob": {
        "children": ["link#smartwatches-trigger"],
        "props":{
            "blockClass":"disclosureMenu"
        }
    },
    "link#smartwatches-trigger":{
		"props":{
			"blockClass":"linkMenuMob",
			"label":"Smartwatches",
			"href":"/tecnologia/wearables"
		}
	},
    "disclosure-content#smartwatches-content-mob": {
        "children": ["menu#category-submenu-smartwatches-mobile"],
        "props":{
            "blockClass":"disclosureMenu"
        }
    },

